[{
  "name": "${name}-container",
  "image": "${uri}",
  "cpu": 0,
  "links": [],
  "portMappings": [
      {
          "name": "${name}-container-${port}",
          "containerPort": ${port},
          "hostPort": ${port},
          "protocol": "tcp",
          "appProtocol": "http"
      }
  ],
  "essential": true,
  "entryPoint": [],
  "command": [],
  "environment": [],
  "environmentFiles": [],
  "mountPoints": [],
  "volumesFrom": [],
  "secrets": [
    {
        "name": "USERNAME",
        "valueFrom": "${secretsmanager_arn}:RDS_USERNAME::"
    },
    {
        "name": "PASSWORD",
        "valueFrom": "${secretsmanager_arn}:RDS_PASSWORD::"
    },
    {
        "name": "HOSTNAME",
        "valueFrom": "${secretsmanager_arn}:RDS_HOSTNAME::"
    },
    {
        "name": "DATABASE",
        "valueFrom": "${secretsmanager_arn}:RDS_DATABASE::"
    }
  ],
  "dnsServers": [],
  "dnsSearchDomains": [],
  "extraHosts": [],
  "dockerSecurityOptions": [],
  "dockerLabels": {},
  "ulimits": [],
  "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "${log_group}",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
      },
      "secretOptions": []
  },
  "systemControls": []
}]